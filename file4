witch case :example 1
echo "swicth case demo start.."
case $1 in
start)
echo "Starting the Sq server"
;;
stop)
echo "stoping the Sq server.."
;;
Restart)
echo "Restaring the Sq server.."
;;
console)
echo "Starting the Sq server in console mode .."
;;
*)
echo "please pass the correct argument.."
echo "usage sh sonar.sh start|stop|Restart|console"
;;
esac
echo "switch to demo user "



